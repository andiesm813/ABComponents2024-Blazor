@page "/avatars_images"

<div class="row-layout avatars-images-container">
    <div class="column-layout group">
        <div class="row-layout group_1">
            <div class="column-layout group_2">
                <div class="row-layout group_3">
                    <IgbAvatar Src="assets/a7f211be67afc061d9571f5b5b5236cd92420e0c.png" Size="SizableComponentSize.Large" Shape="AvatarShape.Circle" class="avatar" avatars_images-scope></IgbAvatar>
                    <IgbAvatar Src="assets/7e86e8b8b495deb8eefcdd3556151a40532211e1.png" Shape="AvatarShape.Rounded" Size="SizableComponentSize.Medium" class="avatar" avatars_images-scope></IgbAvatar>
                    <IgbAvatar Src="assets/d4c387693b4db8c8354a14897762a46dcf820844.png" class="avatar" avatars_images-scope></IgbAvatar>
                </div>
                <p class="typography__subtitle-2 text">
                    Images from Assets
                </p>
            </div>
            <div class="column-layout group_2">
                <div class="row-layout group_3">
                    <IgbAvatar Src="https://images.unsplash.com/photo-1638803040283-7a5ffd48dad5?q=80&w=2592&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D" Size="SizableComponentSize.Large" Shape="AvatarShape.Circle" class="avatar" avatars_images-scope></IgbAvatar>
                    <IgbAvatar Src="https://images.unsplash.com/photo-1640379878948-72b9db349e17?q=80&w=1944&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D" Shape="AvatarShape.Rounded" Size="SizableComponentSize.Medium" class="avatar" avatars_images-scope></IgbAvatar>
                    <IgbAvatar Src="https://images.unsplash.com/photo-1642861937504-62b536fcc1b9?q=80&w=1944&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D" class="avatar" avatars_images-scope></IgbAvatar>
                </div>
                <p class="typography__subtitle-2 text">
                    Images from URL
                </p>
            </div>
        </div>
        <div class="row-layout group_2">
            <IgbList class="list" avatars_images-scope>
                <IgbListItem>
                    <IgbAvatar slot="start" Shape="AvatarShape.Circle" class="avatar" avatars_images-scope></IgbAvatar>
                    <div slot="title">No image</div>
                    <div slot="subtitle">Subtitle...</div>
                </IgbListItem>
                <IgbListItem>
                    <IgbAvatar Src="assets/a06650fce7091f420c440f26201d916f775db29d.png" slot="start" class="avatar" avatars_images-scope></IgbAvatar>
                    <div slot="title">Image from Workspace</div>
                    <div slot="subtitle">Subtitle...</div>
                </IgbListItem>
                <IgbListItem>
                    <IgbAvatar Src="https://images.unsplash.com/photo-1639503611585-1054af5dbfab?q=80&w=2592&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D" slot="start" class="avatar" avatars_images-scope></IgbAvatar>
                    <div slot="title">Image from URL</div>
                    <div slot="subtitle">Subtitle...</div>
                </IgbListItem>
            </IgbList>
            <IgbList class="list" avatars_images-scope>
                <IgbListItem>
                    <IgbAvatar Size="SizableComponentSize.Medium" slot="start" Shape="AvatarShape.Circle" class="avatar" avatars_images-scope></IgbAvatar>
                    <div slot="title">No image</div>
                    <div slot="subtitle">Subtitle...</div>
                </IgbListItem>
                <IgbListItem>
                    <IgbAvatar Src="assets/10a04583112d68e72f71ea3d9a5d02a35bbdf8d8.png" Shape="AvatarShape.Rounded" Size="SizableComponentSize.Medium" slot="start" class="avatar" avatars_images-scope></IgbAvatar>
                    <div slot="title">Image from Workspace</div>
                    <div slot="subtitle">Subtitle...</div>
                </IgbListItem>
                <IgbListItem>
                    <IgbAvatar Src="https://images.unsplash.com/photo-1639628735078-ed2f038a193e?q=80&w=3774&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D" Shape="AvatarShape.Rounded" Size="SizableComponentSize.Medium" slot="start" class="avatar" avatars_images-scope></IgbAvatar>
                    <div slot="title">Image from URL</div>
                    <div slot="subtitle">Subtitle...</div>
                </IgbListItem>
            </IgbList>
            <IgbList class="list" avatars_images-scope>
                <IgbListItem>
                    <IgbAvatar Size="SizableComponentSize.Large" slot="start" Shape="AvatarShape.Circle" class="avatar" avatars_images-scope></IgbAvatar>
                    <div slot="title">No image</div>
                    <div slot="subtitle">Subtitle...</div>
                </IgbListItem>
                <IgbListItem>
                    <IgbAvatar Src="assets/82e19827573e63ebf434524f39a708e6fd21f88a.png" Size="SizableComponentSize.Large" slot="start" Shape="AvatarShape.Circle" class="avatar" avatars_images-scope></IgbAvatar>
                    <div slot="title">Image from Workspace</div>
                    <div slot="subtitle">Subtitle...</div>
                </IgbListItem>
                <IgbListItem>
                    <IgbAvatar Src="https://plus.unsplash.com/premium_photo-1681426472026-60d4bf7b69a1?q=80&w=3880&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D" Size="SizableComponentSize.Large" slot="start" Shape="AvatarShape.Circle" class="avatar" avatars_images-scope></IgbAvatar>
                    <div slot="title">Image from URL</div>
                    <div slot="subtitle">Subtitle...</div>
                </IgbListItem>
            </IgbList>
        </div>
        <div class="row-layout group_4">
            <IgbCard class="card" avatars_images-scope>
                <IgbCardMedia class="media-content" avatars_images-scope>
                    <img src="assets/start-building-dark.svg" class="image" />
                </IgbCardMedia>
                <IgbCardHeader>
                    <div slot="thumbnail">
                        <IgbAvatar Src="assets/Avatar13.png" Size="SizableComponentSize.Large" Shape="AvatarShape.Circle" class="avatar" avatars_images-scope></IgbAvatar>
                    </div>
                    <h3 slot="title">
                        Images from Assets
                    </h3>
                    <h5 slot="subtitle">
                        Subtitle goes here...
                    </h5>
                </IgbCardHeader>
                <IgbCardActions class="actions-content" avatars_images-scope>
                    <IgbButton Variant="ButtonVariant.Flat" slot="start" Size="SizableComponentSize.Large" class="button" avatars_images-scope>
                        Button
                        <IgbRipple></IgbRipple>
                    </IgbButton>
                    <IgbIconButton slot="end" Variant="IconButtonVariant.Flat">
                        <span class="material-icons">
                            favorite
                        </span>
                        <IgbRipple></IgbRipple>
                    </IgbIconButton>
                    <IgbIconButton slot="end" Variant="IconButtonVariant.Flat">
                        <span class="material-icons">
                            bookmark
                        </span>
                        <IgbRipple></IgbRipple>
                    </IgbIconButton>
                    <IgbIconButton slot="end" Variant="IconButtonVariant.Flat">
                        <span class="material-icons">
                            share
                        </span>
                        <IgbRipple></IgbRipple>
                    </IgbIconButton>
                </IgbCardActions>
            </IgbCard>
            <IgbCard class="card" avatars_images-scope>
                <IgbCardMedia class="media-content" avatars_images-scope>
                    <img src="https://images.unsplash.com/photo-1638643391904-9b551ba91eaa?q=80&w=2592&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D" class="image" />
                </IgbCardMedia>
                <IgbCardHeader>
                    <div slot="thumbnail">
                        <IgbAvatar Src="https://images.unsplash.com/photo-1636622433525-127afdf3662d?q=80&w=3864&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D" Size="SizableComponentSize.Large" Shape="AvatarShape.Circle" class="avatar" avatars_images-scope></IgbAvatar>
                    </div>
                    <h3 slot="title">
                        Images From URL
                    </h3>
                    <h5 slot="subtitle">
                        Subtitle goes here...
                    </h5>
                </IgbCardHeader>
                <IgbCardActions class="actions-content" avatars_images-scope>
                    <IgbButton Variant="ButtonVariant.Flat" slot="start" Size="SizableComponentSize.Large" class="button" avatars_images-scope>
                        Button
                        <IgbRipple></IgbRipple>
                    </IgbButton>
                    <IgbIconButton slot="end" Variant="IconButtonVariant.Flat">
                        <span class="material-icons">
                            favorite
                        </span>
                        <IgbRipple></IgbRipple>
                    </IgbIconButton>
                    <IgbIconButton slot="end" Variant="IconButtonVariant.Flat">
                        <span class="material-icons">
                            bookmark
                        </span>
                        <IgbRipple></IgbRipple>
                    </IgbIconButton>
                    <IgbIconButton slot="end" Variant="IconButtonVariant.Flat">
                        <span class="material-icons">
                            share
                        </span>
                        <IgbRipple></IgbRipple>
                    </IgbIconButton>
                </IgbCardActions>
            </IgbCard>
        </div>
    </div>
    <div class="column-layout group">
        <p class="typography__subtitle-2 text">
            Image from Assets
        </p>
        <img src="assets/SketchParser.png" class="image_1" />
        <p class="typography__subtitle-2 text">
            Image from URL
        </p>
        <img src="https://images.unsplash.com/photo-1593085512500-5d55148d6f0d?q=80&w=2334&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D" class="image_2" />
    </div>
</div>

